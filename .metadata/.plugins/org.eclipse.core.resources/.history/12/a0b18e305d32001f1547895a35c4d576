#include "MAX30102.h"

#define ADDRESS 0xAE
#define ADDRESS 0xAE

uint8_t Read_Byte(uint8_t reg)
{
	uint8_t data;
	HAL_I2C_Mem_Read(&hi2c1, ADDRESS, reg, I2C_MEMADD_SIZE_8BIT, &data, 1, HAL_MAX_DELAY)
}

void MAX_Init(void)
{

	return 1;
}
