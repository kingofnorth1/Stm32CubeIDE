#define SDA_Clr() HAL_GPIO_WritePin(GPIOB, SDA_Pin, GPIO_PIN_SET);
#define SDA_Set() HAL_GPIO_WritePin(GPIOB, SDA_Pin, GPIO_PIN_RESET);

#define SDA_Clr() HAL_GPIO_WritePin(GPIOB, SDA_Pin, GPIO_PIN_SET);
#define SDA_Set() HAL_GPIO_WritePin(GPIOB, SDA_Pin, GPIO_PIN_RESET);

