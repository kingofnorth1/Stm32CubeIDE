#include "py32f0xx_hal.h"

void Radio_Init(void)
{
  GPIO_InitTypeDef GPIO_StructInit = {0};
  
  __HAL_RCC_GPIOA_CLK_ENABLE();
  
  GPIO_StructInit.Mode = GPIO_MODE_AF_PP;
  GPIO_StructInit.Pin = GPIO_PIN_3;
  GPIO_StructInit.Pull = GPIO_NOPULL;
  GPIO_StructInit.Speed = GPIO_SPEED_FREQ_MEDIUM;
  HAL_GPIO_Init(GPIOA, &GPIO_StructInit); 
}

void Shnd_Music(void)
{
    HAL_GPIO_WritePin(GPIOA, GPIO_PIN_3, GPIO_PIN_RESET);
}
